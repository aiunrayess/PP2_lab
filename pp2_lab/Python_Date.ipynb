   "source": [
    "from datetime import datetime, timedelta\n",
    "n = int(input())\n",
    "a = datetime.now()\n",
    "\n",
    "b = a - timedelta(days = n)\n",
    "\n",
    "print(\"Current date: \", a.strftime(\"%Y-%m-%d\"))\n",
    "print(f\"Date substracting {n} days: \", b.strftime('%Y-%m-%d'))"
   ]
  }
